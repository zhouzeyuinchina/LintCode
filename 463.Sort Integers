class Solution {
public:
    /**
     * @param A: an integer array
     * @return: nothing
     */
    void sortIntegers(vector<int> &A) {
    for(int i=0;i<A.size();i++)
    {
	    int t=i;
	    for(int j=i+1;j<A.size();j++)
	        {
	        	if(A[j]<A[t]) t=j;
	        	
	        }
	    swap(A[i],A[t]);

}
}
};
